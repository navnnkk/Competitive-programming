#include<bits/stdc++.h>
#define ll long long
#define pb push_back
#define mp make_pair
#define fi first
#define se second
#define loop(i,n) for(int i=0;i<n;i++)
#define in(i,n,a) for(int i=0;i<n;i++) cin>>a[i];
#define nitrous ios_base::sync_with_stdio(false); cin.tie(NULL);
using namespace std;

/*bool compare(const pr<string,int> &a, const pr<string,int> &b)
{
    return a.se > b.se;
}*/

int main()
{
	unsigned ll t,n,digit;
	cin>>t;
	string p = "1",q;

	while(t--)
    {
        q = "1";
        cin>>n;
        if(n == 1)
            cout<<p<<" "<<q<<"\n";
        else
        {
            digit = n / 2;
            for(int i=0;i<digit;i++)
                q += "0";
            cout<<p<<" "<<q<<"\n";
        }
    }
    return 0;
}
